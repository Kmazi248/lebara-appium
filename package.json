    "@wdio/appium-service": "^9.19.2",
    "@wdio/cli": "^9.19.2",
    "@wdio/local-runner": "^9.19.2",
    "@wdio/mocha-framework": "^9.19.2",
    "@wdio/spec-reporter": "^9.19.2",
    "allure-commandline": "^2.34.1",
    "appium": "^3.0.1",
    "appium-uiautomator2-driver": "^5.0.0",
    "cross-env": "^10.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "webdriverio": "^9.19.2"
