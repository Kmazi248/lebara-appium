{
  "name": "lebara-appium",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "wdio": "wdio run ./wdio.conf.ts",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "allure report": "npm run allure:generate && npm run allure:open",
    "appium": "appium",
    "test": "wdio run ./wdio.conf.ts",
    "test:debug": "cross-env WDIO_LOG_LEVEL=debug wdio run ./wdio.conf.ts",
    "test:web": "wdio run ./wdio.conf.ts",
    "ci:test": "cross-env CI=true WDIO_MAX_INSTANCES=1 wdio run ./wdio.conf.ts",
    "ci:report": "allure generate --clean allure-results -o allure-report"
  },
